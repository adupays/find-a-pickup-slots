{
  "name": "find-a-pickup-slots",
  "version": "1.0.0",
  "description": "",
  "main": "src/App.js",
  "scripts": {
    "clear-build-cache": "rm -rf .cache/ dist/",
    "dev": "npm run clear-build-cache && npm run dev-client & npm run dev-server",
    "dev-client": "parcel src/index.html",
    "dev-server": "cross-env NODE_ENV=development nodemon --exec babel-node server/index.js",
    "build": "npm run clear-build-cache && npm run build-client && npm run build-server",
    "build-client": "cross-env BABEL_ENV=client parcel build src/index.html -d dist/client --public-url /dist",
    "build-server": "cross-env BABEL_ENV=server parcel build server/index.js -d dist/server --public-url /dist --target node",
    "start": "node dist/server",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adupays/find-a-pickup-slots.git"
  },
  "author": "Amandine Dupays",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adupays/find-a-pickup-slots/issues"
  },
  "homepage": "https://github.com/adupays/find-a-pickup-slots#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "nodemon": "^2.0.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "2.0.4"
  },
  "dependencies": {
    "@reach/router": "^1.3.3",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-imported-component": "^6.2.1"
  },
  "engines": {
    "node": "~12.16.2"
  }
}
